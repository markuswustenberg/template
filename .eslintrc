extends: 'eslint:recommended'
ecmaFeatures:
  arrowFunctions: true
  experimentalObjectRestSpread: true
  modules: true
  jsx: true
env:
  browser: true
  es6: true
plugins:
  - react
rules:
  indent:
    - 2
    - 2
  linebreak-style:
    - 2
    - unix
  quotes:
    - 2
    - single
  semi:
    - 2
    - never
