{
  "name": "template",
  "version": "0.1.0",
  "description": "A template for web projects.",
  "main": "source/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean && cp static/* build/ && browserify -t babelify source/app.js -o build/app.js",
    "clean": "mkdir -p build && rm -rf build/*",
    "lint": "eslint source tests",
    "test": "browserify -t babelify tests/app.js | node | faucet",
    "watch": "npm run build && watchify -t babelify source/app.js -o build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markuswustenberg/template.git"
  },
  "author": "Markus WÃ¼stenberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markuswustenberg/template/issues"
  },
  "homepage": "https://github.com/markuswustenberg/template#readme",
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babelify": "latest",
    "browserify": "latest",
    "eslint": "latest",
    "eslint-plugin-react": "latest",
    "faucet": "latest",
    "tape": "latest",
    "watchify": "latest"
  }
}
