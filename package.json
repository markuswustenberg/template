  "bugs": {
    "url": "https://github.com/markuswustenberg/template/issues"
  },
  "homepage": "https://github.com/markuswustenberg/template#readme",
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babelify": "^6.2.0",
    "browserify": "^11.0.1",
    "eslint": "^1.2.1",
    "eslint-plugin-react": "^3.3.0",
    "watchify": "^3.3.1"
  }
}
