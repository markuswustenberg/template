{
  "name": "template",
  "version": "0.1.0",
  "description": "A template for web projects.",
  "author": "Markus WÃ¼stenberg",
  "homepage": "https://github.com/markuswustenberg/template",
  "license": "MIT",
  "main": "source/app.js",
  "directories": {
    "lib": "source",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markuswustenberg/template.git"
  },
  "bugs": {
    "url": "https://github.com/markuswustenberg/template/issues"
  },
  "scripts": {
    "browser": "opn http://localhost:8080",
    "build": "npm run clean && npm run copy-static && browserify -t [ babelify --presets [ es2015 react ] ] source/app.js -o build/app.js",
    "clean": "mkdir -p build && rm -rf build/*",
    "copy-static": "cp static/* build/",
    "lint": "eslint source tests",
    "start": "static build",
    "test": "browserify -t [ babelify --presets [ es2015 react ] ] tests/app.js | node | faucet",
    "watch": "npm run clean && npm run copy-static && watchify -v -d -t [ babelify --presets [ es2015 react ] ] source/app.js -o build/app.js"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babelify": "latest",
    "browserify": "latest",
    "eslint": "latest",
    "eslint-plugin-react": "latest",
    "faucet": "latest",
    "node-static": "latest",
    "opn-cli": "latest",
    "tape": "latest",
    "watchify": "latest"
  }
}
