{
  "name": "template",
  "version": "0.1.0",
  "description": "A template for web projects.",
  "author": "Markus WÃ¼stenberg",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run copy-static && NODE_ENV=production browserify -t [ babelify ] source/app.js | uglifyjs - -m -c -o build/app.js",
    "clean": "mkdir -p build && rm -rf build/*",
    "copy-static": "cp -a static/* build/",
    "lint": "eslint source tests",
    "start": "static build",
    "test": "browserify -t [ babelify ] tests/app.js | node | faucet",
    "watch": "npm run clean && npm run copy-static && watchify -v -d -t [ babelify ] source/app.js -o build/app.js"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-2": "latest",
    "babelify": "latest",
    "browserify": "latest",
    "eslint": "latest",
    "eslint-plugin-react": "latest",
    "faucet": "latest",
    "node-static": "latest",
    "tape": "latest",
    "uglify-js": "latest",
    "watchify": "latest"
  }
}
